
// var cannot be used as a Generic type

import java.util.*;
class varGenerics {
	public static void main(String[] args)
	{
		// Generic list using var
		var<var> al = new ArrayList<>();
		
		// add elements
		al.add(10);
		al.add(20);
		al.add(30);
	
		// print the list
		System.out.println(al);
	}
}

// O/P:error: Class names, 'varGenerics', are only accepted if annotation processing is explicitly requested
// 1 error
